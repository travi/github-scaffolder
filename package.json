{
  "name": "@travi/github-scaffolder",
  "description": "VCS scaffolder for projects to be hosted on GitHub",
  "license": "MIT",
  "version": "0.0.0-semantically-released",
  "type": "module",
  "engines": {
    "node": "^16.14 || >=18"
  },
  "author": "Matt Travi <npm@travi.org> (https://matt.travi.org/)",
  "repository": "travi/github-scaffolder",
  "bugs": "https://github.com/travi/github-scaffolder/issues",
  "homepage": "https://npm.im/@travi/github-scaffolder",
  "exports": "./lib/index.js",
  "main": "./lib/index.js",
  "scripts": {
    "clean": "rimraf lib/",
    "lint:js": "eslint . --cache",
    "lint:gherkin": "gherkin-lint",
    "lint:md": "remark --frail .",
    "lint:sensitive": "ban",
    "lint:peer": "npm ls >/dev/null",
    "lint:engines": "ls-engines",
    "lint:publish": "publint --strict",
    "pretest": "run-s build",
    "test": "npm-run-all --print-label --parallel lint:* --parallel test:*",
    "test:unit:base": "DEBUG=any vitest run",
    "test:unit": "cross-env NODE_ENV=test c8 run-s test:unit:base",
    "pretest:integration": "run-s build",
    "test:integration": "run-s 'test:integration:base -- --profile noWip'",
    "test:integration:base": "NODE_OPTIONS=\"--enable-source-maps $NODE_OPTIONS\" cucumber-js test/integration",
    "test:integration:debug": "DEBUG=test,nock.* run-s test:integration",
    "test:integration:wip": "run-s 'test:integration:base -- --profile wip'",
    "test:integration:focus": "run-s 'test:integration:base -- --profile focus'",
    "build": "run-s clean build:*",
    "build:js": "rollup -c",
    "generate:md": "remark . --output",
    "watch": "run-s 'build:js -- --watch'",
    "prepack": "run-s build",
    "prepare": "husky install"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@form8ion/overridable-prompts": "^1.1.0",
    "@form8ion/repository-settings": "^1.0.0-alpha.1",
    "@octokit/rest": "^19.0.0",
    "@travi/cli-messages": "1.1.1",
    "git-config": "0.0.7",
    "octokit-auth-netrc": "^2.0.0"
  },
  "devDependencies": {
    "@cucumber/cucumber": "9.5.1",
    "@travi/any": "2.1.9",
    "@travi/babel-preset": "3.0.108",
    "@travi/eslint-config": "4.0.3",
    "@travi/eslint-config-cucumber": "1.0.6",
    "ban-sensitive-files": "1.10.0",
    "c8": "8.0.1",
    "chai": "4.3.8",
    "chai-as-promised": "7.1.1",
    "commitlint-config-travi": "1.4.35",
    "cross-env": "7.0.3",
    "cz-conventional-changelog": "3.3.0",
    "debug": "4.3.4",
    "gherkin-lint": "4.2.2",
    "http-status-codes": "2.2.0",
    "husky": "8.0.3",
    "jest-when": "3.6.0",
    "js-yaml": "4.1.0",
    "lodash.zip": "4.2.0",
    "ls-engines": "0.9.0",
    "mock-fs": "5.2.0",
    "nock": "13.3.3",
    "npm-run-all2": "6.0.6",
    "publint": "0.2.2",
    "remark": "14.0.3",
    "remark-cli": "11.0.0",
    "remark-preset-lint-travi": "6.0.1",
    "remark-toc": "8.0.1",
    "remark-usage": "10.0.1",
    "rimraf": "5.0.1",
    "rollup": "3.29.0",
    "rollup-plugin-auto-external": "2.0.0",
    "vitest": "0.34.3"
  }
}