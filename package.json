{
  "name": "@travi/github-scaffolder",
  "description": "VCS scaffolder for projects to be hosted on GitHub",
  "version": "0.0.0-semantically-released",
  "main": "lib/index.cjs.js",
  "module": "lib/index.es.js",
  "license": "MIT",
  "repository": "travi/github-scaffolder",
  "bugs": "https://github.com/travi/github-scaffolder/issues",
  "homepage": "https://npm.im/@travi/github-scaffolder",
  "author": "Matt Travi <npm@travi.org> (https://matt.travi.org/)",
  "scripts": {
    "clean": "rimraf lib/",
    "lint:js": "eslint . --cache",
    "lint:md": "remark --frail .",
    "lint:travis": "travis-lint .travis.yml",
    "lint:sensitive": "ban",
    "test": "npm-run-all --print-label --parallel lint:* --parallel test:*",
    "test:unit:base": "mocha --recursive test/unit",
    "test:unit": "nyc run-s test:unit:base",
    "coverage:report": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "build": "run-s clean build:*",
    "build:js": "rollup -c",
    "generate:md": "remark . --output",
    "watch": "run-s 'build:js -- --watch'",
    "prepack": "run-s build"
  },
  "publishConfig": {
    "access": "public"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@travi/any": "^2.0.0",
    "@travi/eslint-config-travi": "^1.8.0",
    "babel-preset-travi": "^1.4.2",
    "babel-register": "^6.26.0",
    "ban-sensitive-files": "^1.9.2",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "codecov": "^3.3.0",
    "commitlint-config-travi": "^1.2.40",
    "cz-conventional-changelog": "^3.0.0",
    "husky": "^3.0.0",
    "mocha": "^6.1.2",
    "npm-run-all": "^4.1.5",
    "nyc": "^14.0.0",
    "remark": "^11.0.0",
    "remark-cli": "^7.0.0",
    "remark-preset-lint-travi": "^1.2.0",
    "remark-toc": "^6.0.0",
    "rimraf": "^3.0.0",
    "rollup": "^1.9.3",
    "rollup-plugin-auto-external": "^2.0.0",
    "sinon": "^7.3.1",
    "travis-lint": "^1.0.0"
  },
  "dependencies": {
    "@octokit/rest": "^16.13.1",
    "@travi/cli-messages": "1.0.1",
    "git-config": "0.0.7",
    "inquirer": "^7.0.0",
    "netrc": "^0.1.4",
    "write-yaml": "^1.0.0"
  }
}
