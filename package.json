{
  "name": "@travi/github-scaffolder",
  "description": "VCS scaffolder for projects to be hosted on GitHub",
  "version": "0.0.0-semantically-released",
  "main": "lib/index.cjs.js",
  "module": "lib/index.es.js",
  "license": "MIT",
  "repository": "travi/github-scaffolder",
  "bugs": "https://github.com/travi/github-scaffolder/issues",
  "homepage": "https://npm.im/@travi/github-scaffolder",
  "author": "Matt Travi <npm@travi.org> (https://matt.travi.org/)",
  "scripts": {
    "clean": "rimraf lib/",
    "lint:js": "eslint . --cache",
    "lint:gherkin": "gherkin-lint",
    "lint:md": "remark --frail .",
    "lint:sensitive": "ban",
    "lint:peer": "npm ls >/dev/null",
    "test": "npm-run-all --print-label build --parallel lint:* --parallel test:*",
    "test:unit:base": "mocha 'src/**/*test.js'",
    "test:unit": "nyc run-s test:unit:base",
    "pretest:integration": "preview",
    "test:integration": "run-s 'test:integration:base -- --profile noWip'",
    "test:integration:base": "cucumber-js test/integration --profile base",
    "test:integration:debug": "DEBUG=test,nock.* run-s test:integration",
    "test:integration:wip": "run-s 'test:integration:base -- --profile wip'",
    "test:integration:focus": "run-s 'test:integration:base -- --profile focus'",
    "coverage:report": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "build": "run-s clean build:*",
    "build:js": "rollup -c",
    "generate:md": "remark . --output",
    "watch": "run-s 'build:js -- --watch'",
    "prepack": "run-s build"
  },
  "publishConfig": {
    "access": "public"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@babel/register": "7.12.1",
    "@travi/any": "2.0.13",
    "@travi/babel-preset": "3.0.33",
    "@travi/eslint-config": "1.0.54",
    "@travi/eslint-config-cucumber": "1.0.5",
    "@travi/eslint-config-mocha": "1.0.8",
    "ban-sensitive-files": "1.9.13",
    "chai": "4.2.0",
    "chai-as-promised": "7.1.1",
    "codecov": "3.8.1",
    "commitlint-config-travi": "1.3.11",
    "cucumber": "6.0.5",
    "cz-conventional-changelog": "3.3.0",
    "debug": "4.2.0",
    "gherkin-lint": "4.1.3",
    "http-status-codes": "2.1.4",
    "husky": "4.3.0",
    "js-yaml": "3.14.0",
    "lodash.zip": "4.2.0",
    "mocha": "8.2.1",
    "mock-fs": "4.13.0",
    "nock": "13.0.5",
    "npm-run-all": "4.1.5",
    "nyc": "15.1.0",
    "package-preview": "3.0.0",
    "remark": "13.0.0",
    "remark-cli": "9.0.0",
    "remark-preset-lint-travi": "1.3.11",
    "remark-toc": "7.0.0",
    "remark-usage": "9.0.0",
    "rimraf": "3.0.2",
    "rollup": "2.33.1",
    "rollup-plugin-auto-external": "2.0.0",
    "sinon": "9.2.1"
  },
  "dependencies": {
    "@form8ion/overridable-prompts": "^1.0.0",
    "@octokit/rest": "^18.0.0",
    "@travi/cli-messages": "1.0.4",
    "git-config": "0.0.7",
    "octokit-auth-netrc": "^1.0.0",
    "write-yaml": "^1.0.0"
  }
}
