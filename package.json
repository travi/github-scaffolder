{
  "name": "@travi/github-scaffolder",
  "description": "VCS scaffolder for projects to be hosted on GitHub",
  "license": "MIT",
  "version": "0.0.0-semantically-released",
  "engines": {
    "node": "^14.15 || >=16"
  },
  "author": "Matt Travi <npm@travi.org> (https://matt.travi.org/)",
  "repository": "travi/github-scaffolder",
  "bugs": "https://github.com/travi/github-scaffolder/issues",
  "homepage": "https://npm.im/@travi/github-scaffolder",
  "main": "lib/index.cjs.js",
  "module": "lib/index.es.js",
  "scripts": {
    "clean": "rimraf lib/",
    "lint:js": "eslint . --cache",
    "lint:gherkin": "gherkin-lint",
    "lint:md": "remark --frail .",
    "lint:sensitive": "ban",
    "lint:peer": "npm ls >/dev/null",
    "lint:engines": "ls-engines",
    "test": "npm-run-all --print-label build --parallel lint:* --parallel test:*",
    "test:unit:base": "mocha 'src/**/*test.js'",
    "test:unit": "cross-env NODE_ENV=test c8 run-s test:unit:base",
    "pretest:integration": "preview",
    "test:integration": "run-s 'test:integration:base -- --profile noWip'",
    "test:integration:base": "cucumber-js test/integration --profile base",
    "test:integration:debug": "DEBUG=test,nock.* run-s test:integration",
    "test:integration:wip": "run-s 'test:integration:base -- --profile wip'",
    "test:integration:focus": "run-s 'test:integration:base -- --profile focus'",
    "build": "run-s clean build:*",
    "build:js": "rollup -c",
    "generate:md": "remark . --output",
    "watch": "run-s 'build:js -- --watch'",
    "prepack": "run-s build",
    "prepare": "husky install"
  },
  "publishConfig": {
    "access": "public"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@form8ion/overridable-prompts": "^1.1.0",
    "@octokit/rest": "^19.0.0",
    "@travi/cli-messages": "1.1.0",
    "git-config": "0.0.7",
    "js-yaml": "^4.0.0",
    "make-dir": "^3.1.0",
    "octokit-auth-netrc": "^2.0.0"
  },
  "devDependencies": {
    "@babel/register": "7.21.0",
    "@cucumber/cucumber": "9.0.1",
    "@travi/any": "2.1.8",
    "@travi/babel-preset": "3.0.96",
    "@travi/eslint-config": "3.0.25",
    "@travi/eslint-config-cucumber": "1.0.6",
    "@travi/eslint-config-mocha": "1.0.19",
    "ban-sensitive-files": "1.9.18",
    "c8": "7.13.0",
    "chai": "4.3.7",
    "chai-as-promised": "7.1.1",
    "commitlint-config-travi": "1.4.26",
    "cross-env": "7.0.3",
    "cz-conventional-changelog": "3.3.0",
    "debug": "4.3.4",
    "gherkin-lint": "4.2.2",
    "http-status-codes": "2.2.0",
    "husky": "8.0.3",
    "lodash.zip": "4.2.0",
    "ls-engines": "0.9.0",
    "mocha": "10.2.0",
    "mock-fs": "5.2.0",
    "nock": "13.3.0",
    "npm-run-all": "4.1.5",
    "package-preview": "4.0.0",
    "remark": "14.0.2",
    "remark-cli": "11.0.0",
    "remark-preset-lint-travi": "5.0.0",
    "remark-toc": "8.0.1",
    "remark-usage": "10.0.1",
    "rimraf": "4.4.1",
    "rollup": "2.79.1",
    "rollup-plugin-auto-external": "2.0.0",
    "sinon": "15.0.2"
  }
}
